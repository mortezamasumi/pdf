{
    "name": "mortezamasumi/pdf-report",
    "description": "Helper package to make pdf reports easier",
    "version": "v1.0",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "laravel",
        "pdf",
        "report"
    ],
    "homepage": "https://github.com/mortezamasumi/pdf-report",
    "authors": [
        {
            "name": "Morteza Masumi",
            "email": "morteza.masumi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "elibyy/tcpdf-laravel": "^11.0"
    },
    "require-dev": {
        "filament/filament": "^3.2",
        "laravel/legacy-factories": "^1.4",
        "livewire/livewire": "^3.5",
        "orchestra/testbench": "^9.6",
        "pestphp/pest": "^3.6",
        "pestphp/pest-plugin-laravel": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0",
        "smalot/pdfparser": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "Mortezamasumi\\PdfReport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mortezamasumi\\PdfReport\\PdfReportServiceProvider"
            ],
            "aliases": {
                "PdfReport": "Mortezamasumi\\PdfReport\\Facades\\PdfReport"
            }
        }
    }
}
